<div id="index-banner" class="parallax-container">
    <div class="section no-pad-bot">
      <div class="container">
        <br>
        <h1 class="header center teal-text text-lighten-2">Coop Scraper</h1>
        <div class="row center">
          <a href="/scrape" id="download-button" class="btn-large waves-effect waves-light teal lighten-1">Scrape the Bottom of the Barrel</a>
        </div>
      </div>
    </div>
    <div class="parallax"><img src="./img/coop1.jpg" alt="Alice Cooper"></div>
  </div>
<br>
<div class="article-container">
{{#each Article}}
<div class="row">
        <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{this.title}}</span>
              <p><a href= "{{this.img}}" target="_blank"> {{this.img}}</a></p>
            </div>
            <div class="card-action">
              <a href="{{this.link}}" target="_blank">Read the Article</a>
              <a href="/notes/{{this.id}}">Make a Note of It</a>
              
            </div>
          </div>
        </div>
      </div> 
{{/each}}
</div>

 <div class="parallax-container valign-wrapper">
    <div class="section no-pad-bot">
      <div class="container">
        <div class="row center">
          <h5 class="header col s12 teal lighten-1">Scrap Happy</h5>
        </div>
      </div>
    </div>
    <div class="parallax"><img src="./img/coop3.jpg" alt="Alice Cooper"></div>
  </div>
